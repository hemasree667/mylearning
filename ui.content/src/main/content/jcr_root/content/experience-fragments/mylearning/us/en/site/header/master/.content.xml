<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
	xmlns:cq="http://www.day.com/jcr/cq/1.0"
	xmlns:jcr="http://www.jcp.org/jcr/1.0"
	xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
	jcr:primaryType="cq:Page">
	<jcr:content
		cq:tags="[]"
		cq:template="/conf/mylearning/settings/wcm/templates/xf-web-variation"
		cq:xfMasterVariation="{Boolean}true"
		cq:xfVariantType="web"
		jcr:primaryType="cq:PageContent"
		jcr:title="Header"
		sling:resourceType="mylearning/components/xfpage">
		<root
			jcr:primaryType="nt:unstructured"
			sling:resourceType="mylearning/components/container"
			layout="simple">
			<navigation
				jcr:primaryType="nt:unstructured"
				sling:resourceType="mylearning/components/navigation"
				collectAllPages="false"
				navigationRoot="/content/mylearning/us/en"
				skipNavigationRoot="false"
				structureDepth="1" />
			<languagenavigation
				jcr:primaryType="nt:unstructured"
				sling:resourceType="mylearning/components/languagenavigation"
				navigationRoot="/content/mylearning/us"
				structureDepth="2" />
			<search
				jcr:primaryType="nt:unstructured"
				sling:resourceType="mylearning/components/search"
				searchRoot="/content/mylearning/us" />
		</root>
	</jcr:content>
</jcr:root>
